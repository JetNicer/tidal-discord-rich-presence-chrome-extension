{
  "name": "Tidal Discord Rich Pressence",
  "description": "An application that tracks Tidal Web Player activity and communicates with an external app to update Discord's Rich Presence accordingly",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background/background.js"
  },
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "/images/icon_16.png",
      "32": "/images/icon_32.png",
      "48": "/images/icon_48.png",
      "128": "/images/icon_128.png"
    }
  },
  "icons": {
    "16": "/images/icon_16.png",
    "32": "/images/icon_32.png",
    "48": "/images/icon_48.png",
    "128": "/images/icon_128.png"
  },
  "options_page": "option/index.html"
}
